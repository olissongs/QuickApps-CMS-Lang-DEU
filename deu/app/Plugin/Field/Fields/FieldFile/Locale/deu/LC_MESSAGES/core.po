#: View/Elements/formatter_from.ctp:8
msgid "Link to file"
msgstr "Link a fichero"

#: View/Elements/formatter_from.ctp:9
msgid "Table of Files"
msgstr "Tabla de Ficheros"

#: View/Elements/formatter_from.ctp:10
msgid "File URL"
msgstr "URL de Fichero"

msgid "Files must be less than <b>%sB</b>."
msgstr "Archivos deben ser menores que <b>%sB</b>."

msgid "Allowed file types: <b>%s</b>."
msgstr "Extensiones admitidas: <b>%s</b>."

msgid "Description"
msgstr "Descripción"

msgid "Remove"
msgstr "Eliminar"

msgid "Attachment"
msgstr "Adjuntos"

msgid "Size"
msgstr "Tamaño"

msgid "Allowed extensions"
msgstr "Extensiones admitidas"

msgid "Comma separated. i.e.: jpg,gif,png"
msgstr "Separados por coma. ej.: jpg,gif,png"

msgid "Number of files"
msgstr "Número de ficheros"

msgid "Maximum number of files users can upload for this field."
msgstr "Número máximo de ficheros que los usuarios pueden subir para este campo."

msgid "Upload folder"
msgstr "Carpeta de subida"

msgid "Optional subdirectory within the upload destination where files will be stored. Do not include preceding or trailing slashes."
msgstr "Subdirectorio opcional en donde los ficheros se almacenarán. No incluya barras (/) iniciales ni finales."

msgid "Enable Description field"
msgstr "Habilitar campo de Descripción"

msgid "The description field allows users to enter a description about the uploaded file."
msgstr "El campo de descripción permite a los usuarios ingresar una breve descripción sobre el fichero cargado."

msgid "Invalid file extension."
msgstr "Extensión de fichero inválida."

msgid "You must upload at least one file."
msgstr "Debe subir al menos un fichero."

